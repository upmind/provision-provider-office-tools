version: '3.3'
services:

  #PHP Service
  provision-provider-pro-email:
    build:
      context: .
      dockerfile: ./.docker/Dockerfile
    container_name: provision-provider-pro-email
    tty: true
    environment:
      SERVICE_NAME: provision-provider-pro-email
      SERVICE_TAGS: dev
    working_dir: /usr/src/lib
    volumes:
      - ./:/usr/src/lib
    networks:
      - provision-provider-pro-email-network

#Docker Networks
networks:
  provision-provider-pro-email-network:
    driver: bridge
